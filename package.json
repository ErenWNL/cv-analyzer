{
  "name": "cv-analyzer",
  "version": "1.0.0",
  "description": "AI-powered CV Analysis Platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "install:all": "npm install && npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErenWNL/cv-analyzer.git"
  },
  "keywords": ["cv", "resume", "analyzer", "ai", "react", "nodejs"],
  "author": "ErenWNL",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ErenWNL/cv-analyzer/issues"
  },
  "homepage": "https://github.com/ErenWNL/cv-analyzer#readme",
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}